{
  "request": {
    "method": "POST",
    "urlPathPattern": "/v1.0/users/[^/]+/calendar/events"
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "jsonBody": {
      "id": "mock-event-id-{{now format='yyyyMMddHHmmssSSS'}}-{{randomValue length=6 type='ALPHANUMERIC'}}",
      "subject": "3 Way Meeting (Non NS) with Jon Smith",
      "start": {
        "dateTime": "2025-09-16T10:00:00",
        "timeZone": "UTC"
      },
      "end": {
        "dateTime": "2025-09-16T10:30:00",
        "timeZone": "UTC"
      },
      "attendees": [
        {
          "emailAddress": {
            "address": "test@example.com",
            "name": "test"
          }
        }
      ]
    }
  }
}
