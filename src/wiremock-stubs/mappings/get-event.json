{
  "request": {
    "method": "GET",
    "urlPathPattern": "/v1.0/users/[^/]+/calendar/events/[^/]+"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "jsonBody": {
      "id": "{{request.pathSegments.[5]}}",
      "subject": "3 Way Meeting (Non NS) with Jon Smith",
      "start": {
        "dateTime": "2026-01-15T10:00:00Z",
        "timeZone": "UTC"
      },
      "end": {
        "dateTime": "2026-01-15T10:30:00Z",
        "timeZone": "UTC"
      },
      "attendees": [
        {
          "emailAddress": {
            "address": "test@example.com",
            "name": "test"
          }
        }
      ]
    }
  }
}